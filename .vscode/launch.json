    "version": "0.2.0",
    "configurations": [
        {
            "name": "privacyIDEA Authenticator - release",
            "type": "dart",
            "request": "launch",
            "program": "lib/mains/main_netknights.dart",
            "args": [
                "--flavor",
                "netknights",
                "--target",
                "lib/mains/main_netknights.dart",
                "--release"
            ]
        },
        {
            "name": "privacyIDEA Authenticator - debug",
            "type": "dart",
            "request": "launch",
            "program": "lib/mains/main_netknights.dart",
            "args": [
                "--flavor",
                "netknights",
                "--target",
                "lib/mains/main_netknights.dart",
                "--debug",
                "--no-enable-impeller"
            ]
        },
        {
            "name": "privacyIDEA Authenticator - internal",
            "type": "dart",
            "request": "launch",
            "program": "lib/mains/main_netknights.dart",
            "args": [
                "--flavor",
                "netknights_debug",
                "--target",
                "lib/mains/main_netknights.dart",
                "--release"
            ]
        },
        {
            "name": "privacyIDEA Authenticator - internal debug",
            "type": "dart",
            "request": "launch",
            "program": "lib/mains/main_netknights.dart",
            "args": [
                "--flavor",
                "netknights_debug",
                "--target",
                "lib/mains/main_netknights.dart",
                "--debug"
            ]
        },
    ]
}